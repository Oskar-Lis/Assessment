<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="UndoAssessment.Views.ApiCall">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Call Success Api"
                    Margin="20"
                Background="green"
                TextColor="White"
                x:Name="successBtn"
                Clicked="onSuccessBtnClicked"/>
            <Button Text="Call Fail Api"
                    Margin="20"
                x:Name="failBtn"
                Background="Red"
                TextColor="White"
                Clicked="onFailBtnClicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>